{
  "name": "esdeka",
  "version": "0.1.14",
  "description": "Communicate between iframe and host",
  "keywords": [
    "react",
    "vanilla",
    "hooks",
    "sdk",
    "iframe",
    "state",
    "widget",
    "app",
    "communicate"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:pixelass/esdeka.git"
  },
  "license": "MIT",
  "author": {
    "name": "Gregor Adams",
    "url": "https://github.com/pixelass"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.mjs",
      "default": "./dist/react.js",
      "types": "./dist/react/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "react": [
        "./dist/react/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/*.js",
    "dist/*.mjs",
    "dist/*.js.map",
    "dist/*.mjs.map",
    "dist/*.d.ts",
    "dist/**/*.js",
    "dist/**/*.mjs",
    "dist/**/*.js.map",
    "dist/**/*.mjs.map",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "build": "yarn rollup",
    "bundlesize": "bundlesize",
    "bundlesize:write": "node ./scripts/bundlesize.mjs",
    "clean": "yarn dlx rimraf dist",
    "dev": "yarn rollup -w",
    "eslint": "eslint 'src/{**/*,*}.{js,ts}'",
    "eslint:fix": "yarn eslint --fix",
    "postinstall": "husky install",
    "lint": "yarn tsc:noEmit && yarn eslint",
    "ncu": "yarn dlx npm-check-updates -u --dep='prod,dev'",
    "ncu:minor": "yarn ncu -t minor",
    "ncu:patch": "yarn ncu -t patch",
    "prepublishOnly": "yarn clean && yarn build && yarn tsc:emitOnly",
    "prettier": "prettier 'src/{**/*,*}.ts'",
    "release": "yarn dlx standard-version@9",
    "rollup": "rollup -c",
    "spj": "yarn dlx sort-package-json package.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "toc": "yarn dlx markdown-toc README.md -i",
    "tsc:emitOnly": "tsc --emitDeclarationOnly --project tsconfig.build.json",
    "tsc:noEmit": "tsc --noEmit",
    "unimported": "yarn dlx unimported"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@jest/globals": "^29.2.2",
    "@swc/core": "^1.3.14",
    "@swc/jest": "^0.2.22",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.2",
    "@types/node": "^16.11.45",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "bundlesize": "^0.18.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^44.0.2",
    "execa": "^6.1.0",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "jest-config": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.2.5",
    "rollup-plugin-swc3": "^0.7.0",
    "tslib": "^2.4.1",
    "type-fest": "^3.1.0",
    "typescript": "^4.8.4"
  },
  "optionalDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "packageManager": "yarn@3.2.0",
  "engines": {
    "node": "^16 | ^17 | ^18"
  }
}
